<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imgHome"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@android:drawable/ic_menu_revert"
            android:tooltipText="Home" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Home" />

        <ImageButton
            android:id="@+id/imgPropria"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/ic_menu_edit"
            android:tooltipText="Colonna propria" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Propria" />

        <ImageButton
            android:id="@+id/imgAltri"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/ic_menu_search"
            android:tooltipText="Colonne Altri" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Altri" />

        <ImageButton
            android:id="@+id/imgClassifiche"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/ic_menu_myplaces"
            android:tooltipText="Classifiche" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Classif." />

        <ImageButton
            android:id="@+id/imgCoppe"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/star_big_off"
            android:tooltipText="Coppe" />

        <TextView
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Coppe" />

        <ImageButton
            android:id="@+id/imgResoconto"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/ic_menu_info_details"
            android:tooltipText="Resoconto" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Resoc." />

        <ImageButton
            android:id="@+id/imgMessaggi"
            android:layout_width="match_parent"
            android:layout_margin="3dp"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/ic_dialog_email"
            android:tooltipText="Amministrazione" />

        <TextView
            android:id="@+id/textViewA3"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Mess." />

        <ImageButton
            android:id="@+id/imgProfilo"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/sconosciuto"
            android:tooltipText="Profilo" />

        <TextView
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Profilo" />

        <ImageButton
            android:id="@+id/imgAmministrazione"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:background="@android:drawable/stat_notify_error"
            android:tooltipText="Amministrazione" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Ammin." />

        <ImageButton
            android:id="@+id/imgAbout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/about"
            android:paddingTop="15dp"
            android:tooltipText="Uscita" />

        <TextView
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="About" />

        <ImageButton
            android:id="@+id/imgUscita"
            android:layout_width="match_parent"
            android:paddingTop="15dp"
            android:layout_height="60dp"
            android:layout_marginTop="5dp"
            android:background="@android:drawable/ic_notification_clear_all"
            android:tooltipText="Uscita" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:shadowColor="@color/blu"
            android:gravity="center"
            android:textSize="12dp"
            android:textColor="@color/bianco"
            android:layout_height="wrap_content"
            android:text="Uscita" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>